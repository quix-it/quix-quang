{
  "compileOnSave": false,
  "compilerOptions": {
    "moduleResolution": "node",
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "module": "esnext",
    "importHelpers": true,
    "skipLibCheck": true,
    "target": "ES2022",
    "typeRoots": [
      "node_modules/@types"
    ],
    "types": [
      "google.maps",
      "googlemaps"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "quang": [
        "dist/quang/quang",
        "dist/quang"
      ],
      "quang-auth": [
        "dist/quang-auth/quang-auth",
        "dist/quang-auth"
      ],
      "quang-keycloak": [
        "dist/quang-keycloak/quang-keycloak",
        "dist/quang-keycloak"
      ],
      "quang-core": [
        "dist/quang-core/quang-core",
        "dist/quang-core"
      ],
      "quang-calendar": [
        "dist/quang-calendar/quang-calendar",
        "dist/quang-calendar"
      ],
      "quang-date": [
        "dist/quang-date/quang-date",
        "dist/quang-date"
      ],
      "quang-chart": [
        "dist/quang-chart/quang-chart",
        "dist/quang-chart"
      ],
      "quang-event": [
        "dist/quang-event/quang-event",
        "dist/quang-event"
      ],
      "quang-media": [
        "dist/quang-media/quang-media",
        "dist/quang-media"
      ],
      "quang-map": [
        "dist/quang-map/quang-map",
        "dist/quang-map"
      ],
      "quang-components": [
        "dist/quang-components/quang-components",
        "dist/quang-components"
      ],
      "qunag-dialog": [
        "dist/qunag-dialog/qunag-dialog",
        "dist/qunag-dialog"
      ],
      "quang-dialog": [
        "dist/quang-dialog/quang-dialog",
        "dist/quang-dialog"
      ],
      "quang-utility": [
        "dist/quang-utility/quang-utility",
        "dist/quang-utility"
      ],
      "quang-kitchen-sink": [
        "dist/quang-kitchen-sink/quang-kitchen-sink",
        "dist/quang-kitchen-sink"
      ]
    },
    "useDefineForClassFields": false
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "enableIvy": false,
    "allowEmptyCodegenFiles": false,
    "strictInjectionParameters": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "importHelpers": true
  }
}
