{
  "name": "quix-quang",
  "version": "19.0.16",
  "scripts": {
    "ng": "ng",
    "start:playground": "ng serve playground --open",
    "build:quang": "ng build --project quang",
    "prepare": "husky",
    "build-publish": "npm run build:quang && cd dist/quang && npm publish",
    "build-publish:prerelease": "npm run build:quang && cd dist/quang && npm publish --tag prerelease",
    "sync-version": "jq '.version = \"'$npm_package_version'\"' projects/quang/package.json > temp.json && mv temp.json projects/quang/package.json",
    "version": "npm run sync-version && git add -A",
    "postversion": "git push && git push --tags",
    "publish:prerelease": "npm version prerelease -m \"build(prerelease): version %s\" && npm run build-publish:prerelease",
    "publish:patch": "npm version patch -m \"build(patch): version %s\" && npm run build-publish",
    "publish:minor": "npm version minor -m \"build(minor): version %s\" && npm run build-publish",
    "publish:major": "npm version major -m \"build(major): version %s\" && npm run build-publish",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "format": "npx prettier . -w --ignore-unknown",
    "lint-and-format": "npm run lint:fix && npm run format"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.0.3",
    "@angular/cdk": "^19.0.2",
    "@angular/common": "^19.0.3",
    "@angular/compiler": "^19.0.3",
    "@angular/core": "^19.0.3",
    "@angular/forms": "^19.0.3",
    "@angular/platform-browser": "^19.0.3",
    "@angular/platform-browser-dynamic": "^19.0.3",
    "@angular/router": "^19.0.3",
    "@capacitor/app": "^6.0.0",
    "@capacitor/browser": "^6.0.1",
    "@capacitor/core": "^6.1.1",
    "@jsverse/transloco": "^7.5.0",
    "@ngrx/signals": "^19.0.0",
    "air-datepicker": "^3.5.0",
    "angular-oauth2-oidc": "^19.0.0",
    "angular-svg-icon": "^19.0.0",
    "bootstrap": "^5.3.3",
    "date-fns": "^3.6.0",
    "rxjs": "~7.8.0",
    "suneditor": "^2.46.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.0.3",
    "@angular/cli": "^19.0.3",
    "@angular/compiler-cli": "^19.0.3",
    "@capacitor/cli": "^6.1.1",
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@eslint/js": "^9.21.0",
    "@trivago/prettier-plugin-sort-imports": "^5.0.0",
    "@types/node": "^20.16.11",
    "angular-eslint": "19.0.2",
    "eslint": "^9.18.0",
    "eslint-config-love": "^117.0.0",
    "husky": "^9.1.2",
    "lint-staged": "^15.2.10",
    "ng-lint-staged": "^12.0.4",
    "ng-packagr": "^19.0.1",
    "prettier": "^3.4.2",
    "prettier-plugin-organize-attributes": "^1.0.0",
    "typescript": "~5.7.3",
    "typescript-eslint": "^8.25.0"
  }
}
