{
  "name": "quix-quang",
  "version": "17.0.0-test-auth-mobile.1",
  "scripts": {
    "ng": "ng",
    "start:playground": "ng serve playground --open",
    "build:quang": "ng build --project quang",
    "prepare": "husky",
    "build-publish": "npm run build:quang && cd dist/quang && npm publish --tag test-auth-mobile",
    "sync-version": "jq '.version = \"'$npm_package_version'\"' projects/quang/package.json > temp.json && mv temp.json projects/quang/package.json",
    "version": "npm run sync-version && git add -A",
    "postversion": "git push && git push --tags",
    "publish:prerelease": "npm version prerelease -m \"build(prerelease): version %s\"",
    "publish:patch": "npm version patch -m \"build(patch): version %s\"",
    "publish:minor": "npm version minor -m \"build(minor): version %s\"",
    "publish:major": "npm version major -m \"build(major): version %s\""
  },
  "private": true,
  "dependencies": {
