{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "quix-core-components": {
      "projectType": "library",
      "root": "projects/quix-core-components",
      "sourceRoot": "projects/quix-core-components/src",
      "prefix": "quix",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/quix-core-components/tsconfig.lib.json",
            "project": "projects/quix-core-components/ng-package.json",
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/quix-core-components/src/test.ts",
            "tsConfig": "projects/quix-core-components/tsconfig.spec.json",
            "karmaConfig": "projects/quix-core-components/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/quix-core-components/tsconfig.lib.json",
              "projects/quix-core-components/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "quix-core-authentication": {
      "projectType": "library",
      "root": "projects/quix-core-authentication",
      "sourceRoot": "projects/quix-core-authentication/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/quix-core-authentication/tsconfig.lib.json",
            "project": "projects/quix-core-authentication/ng-package.json",
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/quix-core-authentication/src/test.ts",
            "tsConfig": "projects/quix-core-authentication/tsconfig.spec.json",
            "karmaConfig": "projects/quix-core-authentication/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/quix-core-authentication/tsconfig.lib.json",
              "projects/quix-core-authentication/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "quang": {
      "projectType": "library",
      "root": "projects/quang",
      "sourceRoot": "projects/quang/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/quang/tsconfig.lib.json",
            "project": "projects/quang/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/quang/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/quang/src/test.ts",
            "tsConfig": "projects/quang/tsconfig.spec.json",
            "karmaConfig": "projects/quang/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/quang/tsconfig.lib.json",
              "projects/quang/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "quix-core-components",
  "cli": {
    "analytics": "4c69ff4f-b242-4742-a667-c15cf0e09a78"
  }
}
